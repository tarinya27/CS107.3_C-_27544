using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp4
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int num1, num2, sum, sub, mul;
            double div;

            Console.Write("Enter two values for num1 and num2:");
            num1=int.Parse(Console.ReadLine());
            num2=int.Parse(Console.ReadLine());

            sum=num1 + num2;
            sub=num1 - num2;
            mul=num1 * num2;
            div = num1 / num2;

            Console.WriteLine("Sum of the values:" + sum);
            Console.WriteLine("Subtraction of the values:" + sub);
            Console.WriteLine("Multipication of the values:" + mul);
            Console.WriteLine("Division of the values:" + div);

            Console.ReadKey();
        }
    }
}
